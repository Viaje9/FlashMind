{
  "mcpServers": {
    "playwright-test": {
      "type": "stdio",
      "command": "node",
      "args": [
        "e2e/node_modules/@playwright/test/cli.js",
        "run-test-mcp-server"
      ]
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--browserUrl",
        "http://127.0.0.1:9222"
      ]
    }
  }
}