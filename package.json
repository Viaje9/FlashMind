{
  "name": "flashmind",
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "scripts": {
    "dev": "pnpm --filter web start & pnpm --filter api start:dev",
    "dev:web": "pnpm --filter ./apps/web start",
    "dev:api": "pnpm --filter ./apps/api start:dev",
    "dev:docs": "pnpm --filter ./apps/docs-viewer dev",
    "build:docs": "pnpm --filter ./apps/docs-viewer build",
    "build:web": "pnpm --filter ./apps/web build",
    "build:api": "pnpm --filter ./apps/api build",
    "storybook:web": "pnpm --filter ./apps/web storybook",
    "build-storybook:web": "pnpm --filter ./apps/web build-storybook",
    "test:e2e": "pnpm --filter ./e2e test",
    "test:e2e:ui": "pnpm --filter ./e2e test:ui",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "typescript": "^5.9.3"
  }
}
